{
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "npm run lint:eslint & npm run lint:stylelint",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,sass,less,vue}\" --fix",
    "cz": "git cz"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "antd": "^4.16.5",
    "classnames": "^2.3.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/babel__core": "^7.1.14",
    "@types/ejs": "^3.0.6",
    "@types/graphql": "^14.5.0",
    "@types/html-minifier-terser": "^5.1.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-legacy": "^1.4.3",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.2",
    "redux-logger": "^3.0.6",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.8",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1992106/vite-react.git"
  },
  "homepage": "https://github.com/1992106/vite-react",
  "keywords": [
    "react",
    "vite",
    "vite-react"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1992106/vite-react/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,sass,less}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
